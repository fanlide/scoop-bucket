            "32bit": {
                "url": "https://resource.u-tools.cn/currentversion/uTools-$version-ia32.exe#/uTools.7z"
            }
        }
    },
    "architecture": {
        "64bit": {
            "url": "https://resource.u-tools.cn/currentversion/uTools-0.8.6-beta.exe#/uTools.7z",
            "hash": "fb6ebb27cf92d417d72282220f7e17f4426d25675032f07d1ea9e5bdafa52e26",
            "installer": {
                "script": "7z x $original_dir/PLUGINSDIR/app-64.7z -o\"$original_dir\""
            }
        },
        "32bit": {
            "url": "https://resource.u-tools.cn/currentversion/uTools-0.8.6-beta-ia32.exe#/uTools.7z",
            "hash": "066df0a64e677c46aff0a3bb8bc4f3f302332041bb33361848ac5cde3ca22baa",
            "installer": {
                "script": "7z x $original_dir/PLUGINSDIR/app-32.7z -o\"$original_dir\""
            }
        }
    },
    "post_install": "rm $original_dir/PLUGINSDIR -recurse",
    "shortcuts": [
        [
            "uTools.exe",
            "uTools"
        ]
    ]
}
